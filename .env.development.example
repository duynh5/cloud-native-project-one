# IoT Temperature Monitoring System - Development Environment Variables

# ============================================
# AWS/SQS Configuration
# ============================================
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test

# ============================================
# SQS Configuration
# ============================================
SQS_ENDPOINT=http://127.0.0.1:4566
TELEMETRY_QUEUE_URL=http://127.0.0.1:4566/000000000000/ShipTelemetryQueue
EVENTS_QUEUE_URL=http://127.0.0.1:4566/000000000000/AlertEventsQueue

# ============================================
# Redis Configuration
# ============================================
REDIS_URL=redis://127.0.0.1:6379

# ============================================
# PostgreSQL Configuration
# ============================================
DATABASE_URL=postgres://user:password@127.0.0.1:5432/ship_db

# ============================================
# Ingestor Service Configuration
# ============================================
INGESTOR_PORT=3000
INGESTOR_HOST=0.0.0.0
INGESTOR_URL=http://127.0.0.1:3000

# ============================================
# Worker Configuration
# ============================================
WORKER_POLL_INTERVAL=20
WORKER_MAX_MESSAGES=10
TREND_WINDOW_MINUTES=5

# ============================================
# Executor Configuration
# ============================================
EXECUTOR_POLL_INTERVAL=20
EXECUTOR_MAX_MESSAGES=10

# ============================================
# Notification Configuration (Future)
# ============================================
EMAIL_NOTIFICATIONS_ENABLED=false
EMAIL_FROM=alerts@shipmonitoring.com
EMAIL_TO=operations@shipmonitoring.com

WEBHOOK_ENABLED=false
WEBHOOK_URL=
